    "bootstrap": "^5.3.2",
    "del": "^6.1.1",
    "eslint": "^8.50.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "highcharts": "^11.1.0",
    "laravel-mix": "^6.0.49",
