{
  "devDependencies": {
    "bootstrap": "^4",
    "copyfiles": "^1",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-minify": "^3.1.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.0.2",
    "highcharts": "^6.2.0",
    "jquery": "^3",
    "leaflet": "^1",
    "leaflet.markercluster": "^1",
    "node-sass": "^4.9.4",
    "npm-run-all": "^4",
    "onchange": "^5",
    "sass": "^1.14.3",
    "tablesorter": "^2",
    "uglify-js": "^3"
  },
  "scripts": {
    "postinstall": "npm run copy && npm run build",
    "copy": "npm run copy-bootstrap && npm run copy-jquery && npm run copy-highcharts && npm run copy-leaflet && npm run copy-tablesorter",
    "copy-bootstrap": "copyfiles -f node_modules/bootstrap/dist/js/bootstrap.min.js public/lib",
    "copy-jquery": "copyfiles -f node_modules/jquery/dist/jquery.min.js public/lib",
    "copy-highcharts": "copyfiles -f node_modules/highcharts/highcharts.js public/lib/",
    "copy-leaflet": "npm run copy-leaflet-js && npm run copy-leaflet-images && npm run copy-leaflet.markercluster",
    "copy-leaflet-js": "copyfiles -f node_modules/leaflet/dist/leaflet.css node_modules/leaflet/dist/leaflet.js  public/lib",
    "copy-leaflet-images": "copyfiles -f node_modules/leaflet/dist/images/layers.png node_modules/leaflet/dist/images/layers-2x.png public/lib/images",
    "copy-leaflet.markercluster": "copyfiles -f node_modules/leaflet.markercluster/dist/leaflet.markercluster.js node_modules/leaflet.markercluster/dist/MarkerCluster.css node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css public/lib/",
    "copy-tablesorter": "copyfiles -f node_modules/tablesorter/dist/js/jquery.tablesorter.min.js public/lib",
    "build": "gulp",
    "watch": "gulp watch"
  }
}
