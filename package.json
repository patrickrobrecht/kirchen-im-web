{
  "private": true,
  "devDependencies": {
    "bootstrap": "^5.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "highcharts": "^9.3.0",
    "jquery": "^3.6.0",
    "laravel-mix": "^6.0.11",
    "laravel-mix-make-file-hash": "^2.2.0",
    "leaflet": "^1.7.1",
    "leaflet.markercluster": "^1.5.3",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.43.3",
    "sass-loader": "^12.2.0",
    "stylelint": "^14.0.0",
    "stylelint-config-standard-scss": "^2.0.0",
    "tablesorter": "^2.31.3"
  },
  "scripts": {
    "postinstall": "npm run build-dev",
    "build": "npm run build-dev",
    "build-dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "build-production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "csCSS": "stylelint resources/css/*.scss",
    "fixCSS": "stylelint resources/css/*.scss --fix",
    "csJS": "eslint resources/js/*.js webpack.mix.js",
    "fixJS": "eslint resources/js/*.js webpack.mix.js --fix",
    "watch": "npm run build-dev -- --watch",
    "watch-poll": "npm run watch -- --watch-poll"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "semi": 0
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard-scss"
    ]
  }
}
