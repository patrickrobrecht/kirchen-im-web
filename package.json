    "bootstrap": "^5.2.3",
    "del": "^6.1.1",
    "eslint": "^8.28.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "highcharts": "^10.3.2",
    "laravel-mix": "^6.0.49",
